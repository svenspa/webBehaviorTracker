{
	"manifest_version": 2,
	"name": "webBehaviorTracker",
	"description": "Track your own web behavior.",
	"version": "0.1",
	"author": "Sven Spa & Roy Theussing",
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Track your own web behavior."
	},
	"permissions": ["tabs", "activeTab", "http://127.0.0.1:5000/*", "storage"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	}
}